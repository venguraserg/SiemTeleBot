<Window x:Class="SiemTeleBot_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SiemTeleBot_WPF"
        mc:Ignorable="d"
        Title="SiemTeleBot" Height="450" Width="800">
    <Grid Margin="0,0,0,0">

        <Button Content="Button" HorizontalAlignment="Left" Margin="719,22,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <ListBox x:Name="ListDbView" 
                 HorizontalAlignment="Left" 
                 Height="329" Margin="35,0,0,0" 
                 VerticalAlignment="Center" Width="704">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel>
                        <TextBlock Text ="{Binding Name}"/>


                    </StackPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>



        </ListBox>


    </Grid>
</Window>
